https://leetcode.com/problems/divide-array-into-equal-pairs/
class Solution {
public:
    bool divideArray(vector<int>& nums) {
        map<int,int>mp;
        for(int i=0;i<nums.size();i++){
            mp[nums[i]]++;
            if(mp[nums[i]]==2){
                return true;
            }

        }
        return false;
        
    }
};
